{
  "name": "embr-syntax",
  "displayName": "Embr Syntax",
  "description": "Syntax highlighting for the Embr programming language",
  "version": "0.11.0",
  "publisher": "Dishpit",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "embr",
        "aliases": ["Embr", "embr"],
        "extensions": [".mbr"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "embr",
        "scopeName": "source.embr",
        "path": "./syntaxes/embr.tmLanguage.json"
      }
    ]
  }
}
